  },
  "dependencies": {
    "@vinxi/server-functions": "^0.3.3",
    "es-module-lexer": "^1.5.4",
    "preact-render-to-string": "^6.5.5",
    "preact-router": "^4.1.2",
    "radix3": "^1.1.2"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.8.2",
    "preact": "^10.22.1",
    "typescript": "^5.5.3",
    "vinxi": "^0.3.11"
  }
