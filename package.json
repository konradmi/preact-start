{
  "name": "preact-start",
  "version": "0.0.2",
  "type": "module",
  "description": "Minimal Preact metaframework",
  "repository": "git@github.com:konradmi/preact-start.git",
  "author": "Konrad Miekus <miekus.konrad@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
