{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "examples:blog:dev": {
      "dependsOn": ["preact-start#dev", "blog#dev"]
    },
    "examples:counter:dev": {
      "dependsOn": ["preact-start#dev", "counter#dev"],
      "outputs": []
    },
    "examples:api-server:dev": {
      "dependsOn": ["preact-start#dev", "api-server#dev"],
      "outputs": []
    },
    "preact-start#dev": {
      "outputs": []
    },
    "preact-start#build": {
      "outputs": []
    },
    "counter#dev": {
      "dependsOn": ["^preact-start#build"],
      "outputs": []
    },
    "api-server#dev": {
      "dependsOn": ["^preact-start#build"],
      "outputs": []
    },
    "blog#dev": {
      "dependsOn": ["^preact-start#build"],
      "outputs": []
    }
  }
}
